        <!--<exception-type>java.lang.Exception</exception-type>-->
        <!--<location>/WEB-INF/jsp/error/error.jsp</location>-->
    <!--</error-page>-->
  <!-- security begin-->
  <filter>
    <filter-name>CAS Single Sign Out Filter</filter-name>
    <filter-class>org.jasig.cas.client.session.SingleSignOutFilter</filter-class>
  </filter>
  <filter>
    <filter-name>CAS Filter</filter-name>
    <filter-class>com.koolearn.security.client.core.MyCASFilter</filter-class>
  </filter>
  <filter>
    <filter-name>springSecurityFilterChain</filter-name>
    <filter-class>com.koolearn.security.client.core.MyDelegatingFilterProxy</filter-class>
    <init-param>
      <param-name>server-host</param-name>
      <param-value></param-value>
    </init-param>
    <init-param>
      <param-name>url-pattern</param-name>
      <param-value></param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CAS Single Sign Out Filter</filter-name>
    <url-pattern>/operation/core/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CAS Filter</filter-name>
    <url-pattern>/operation/core/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>springSecurityFilterChain</filter-name>
    <url-pattern>/operation/core/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CAS Single Sign Out Filter</filter-name>
    <url-pattern>/security/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>springSecurityFilterChain</filter-name>
    <url-pattern>/security/*</url-pattern>
  </filter-mapping>
   <!--security end-->
  <session-config>
    <session-timeout>120</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>/index.jsp</welcome-file>
  </welcome-file-list>
</web-app>